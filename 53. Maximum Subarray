class Solution {
    public int maxSubArray(int[] nums) {
        int i=0;
        int currsum=nums[i];
        int maxsum=nums[i];
        i=1;
        while(i<nums.length)
        {
            currsum=Math.max(nums[i],nums[i]+currsum);
            maxsum=Math.max(maxsum,currsum);
            i++;
        }
        return maxsum; 
    }
}
